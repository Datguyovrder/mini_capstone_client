 <% @products.each do |product| %>
   <h2><a href="/client/products/<%= product["id"] %>"><%= product["name"] %></a></h2>
   <h3>ID: <%= product["id"] %></h3>
   <h3>Price: <%= number_to_currency(product["price"]) %><h3>
   <h4>Description: <%= product["description"] %></h4>
   <h4>Image URL: <%= product["images"] %></h4>
   <br>
 <% end %> 